        <div role="tabpanel" class="tab-pane fade in active" id="ocenione">
          <table id="answer_rated" class="table table-striped">
            <thead>
              <tr>
                <th>Student</th>
                <th>Grupa</th>
                <th>Ocena</th>
                <th>Lekcja</th>
                <th>Ćwiczenie</th>
                <th>Godzina przysłania</th>
                <th colspan="3"></th>
              </tr>
            </thead>
            <tbody>
              <% @read_answers.each do |answer| %>
                <tr>
                  <%@exercise = Exercise.find(answer.exercise_id)%>
                  <%@result = Result.find_by(exercise_id: @exercise.id, student_id: answer.student.id)%>
                  <td><%= answer.student.name %> <%= answer.student.lastname %></td>
                  <td><%= answer.student.group.name %></td>
                  <td><%= @result.earned_points%></td>
                  <td><%= @exercise.level %></td> 
                  <td><%= @exercise.number %></td>
                  <td><%= answer.updated_at.in_time_zone("Warsaw").strftime("%Y-%m-%d %H:%M:%S")%></td>
                  <td><%= link_to 'Pokaż', answer , :class =>"btn btn-primary"%></td>
                </tr>
              <% end %>
            </tbody>
          </table>
        </div>
